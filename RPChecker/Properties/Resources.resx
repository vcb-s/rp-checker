    <value>import vapoursynth as vs
import functools
from vapoursynth import core

srcpath = R"%File1"
rippath = R"%File2"
src = core.lsmas.LWLibavSource(srcpath)
rip = core.lsmas.LWLibavSource(rippath)

if rip.format.subsampling_h == 1 and rip.format.subsampling_w == 1:
    format = vs.YUV420P8
elif rip.format.subsampling_h == 0 and rip.format.subsampling_w == 0:
    format = vs.YUV444P8
else:
    assert False, "Only Support YUV420 and YUV444 format!"
    
if not (rip.format.bits_per_sample == 8): rip = rip.resize.Spline64(format=format, dither_type='none')
if not (src.format.bits_per_sample == 8): src = src.resize.Spline64(format=format, dither_type='none')

if src.width != rip.width or src.height != rip.height:
    src = src.resize.Spline36(rip.width, rip.height)

srcy, srcu, srcv = src.std.SplitPlanes()
ripy, ripu, ripv = rip.std.SplitPlanes()

# runtime function to print psnr value to console.
def callback(n, f, clip):
    print(f"RPCOUT: {n} {f[0].props.PlanePSNR} {f[1].props.PlanePSNR} {f[2].props.PlanePSNR}", flush=True)
    return clip

cmpy = core.complane.PSNR(ripy, srcy)
cmpu = core.complane.PSNR(ripu, srcu)
cmpv = core.complane.PSNR(ripv, srcv)

cmp = core.std.FrameEval(rip,functools.partial(callback, clip=rip), prop_src=[cmpy, cmpu, cmpv], clip_src=[cmpy, cmpu, cmpv])
cmp.set_output()</value>
    <comment>计算PSNR的vpy模板</comment>
  </data>
